<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Commit Online to view later"
            android:id="@+id/textView62" android:layout_gravity="center_horizontal"/>
    <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Commit Online"
            android:onClick="CommitOnline"
            android:id="@+id/button11"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignEnd="@+id/textView46" android:layout_alignTop="@+id/button13"
            android:layout_gravity="center_horizontal"/>
    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="346dp" android:layout_gravity="center_horizontal">
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Large rre"
                android:id="@+id/textView46"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true"/>
        <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Home"
                android:onClick="Home"
                android:id="@+id/button13"
                android:layout_alignParentBottom="true"
                android:layout_alignParentStart="true" android:layout_alignParentEnd="true"/>
    </RelativeLayout>
</LinearLayout>